{
  "name": "1432775-typoteka-3",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.js",
  "scripts": {
    "start": "concurrently \"npm run start-back\" \"npm run start-front\"",
    "start::dev": "concurrently --names \"REST,FRONT\" --prefix-colors \"blue,#d925c4\" --kill-others-on-fail \"npm run start-back::dev\" \"npm run start-front::dev\"",
    "start-back": "cross-env NODE_ENV=production node src/service/service.js --server",
    "start-back::dev": "cross-env NODE_ENV=development nodemon src/service/service.js --server -L",
    "start-front": "cross-env NODE_ENV=production node ./src/express/express.js",
    "start-front::dev": "cross-env NODE_ENV=development nodemon ./src/express/express.js -L -e js,pug,json",
    "test": "npm run eslint && npm run jest",
    "eslint": "eslint --ext .js src/",
    "jest": "jest",
    "filldb": "node src/service/service.js --filldb 20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ibarsuk/1432775-typoteka-3.git"
  },
  "author": "IBars",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ibarsuk/1432775-typoteka-3/issues"
  },
  "homepage": "https://github.com/Ibarsuk/1432775-typoteka-3#readme",
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-htmlacademy": "0.6.0",
    "eslint-plugin-jest": "25.2.2",
    "jest": "27.3.1",
    "nodemon": "2.0.13",
    "pino-pretty": "7.1.0",
    "sqlite3": "5.0.2",
    "supertest": "6.1.6"
  },
  "dependencies": {
    "axios": "0.24.0",
    "bcrypt": "5.0.1",
    "chalk": "4.1.2",
    "concurrently": "7.0.0",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "joi": "17.4.2",
    "jsonwebtoken": "8.5.1",
    "multer": "1.4.3",
    "nanoid": "3.1.30",
    "pg": "8.7.1",
    "pino": "7.0.5",
    "pug": "3.0.2",
    "sequelize": "6.12.0-alpha.1",
    "socket.io": "4.4.1"
  }
}
